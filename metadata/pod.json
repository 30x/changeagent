{
	"apiVersion": "v1",
	"kind": "Pod",
	"metadata": {
		"name": "changeagent",
    "labels": {
        "app": "changeagent"
    },
		"annotations": {
      "publicPaths": "8080:/changes",
			"projectcalico.org/policy": "allow tcp"
		}
	},
	"spec": {
		"imagePullSecrets": [{
			"name": "ecr-us-east-1-key"
		}],
		"containers": [{
			"name": "changeagent",
			"image": "changeagent:latest",
			"imagePullPolicy": "Never",
			"ports": [{
				"containerPort": 8080
			}]
		}]
	}
}
